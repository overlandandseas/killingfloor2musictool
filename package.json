{
  "name": "killing-floor-2-music-tool",
  "version": "0.0.9",
  "description": "Simple GUI tool (windows only) to change and insert your own custom music in Killing Floor 2.",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "clean": "rm -rf ./dist && rm ./data/updated_data.json",
    "build": "npm run clean && electron-packager ./ \"ProjectDemonHunterHunter\" --out=dist/win --platform=win32 --arch=x64 --version=0.36.0 --icon=assets/win/kfthingy.ico"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/overlandandseas/projectdemonhunterhunter.git"
  },
  "keywords": [
    "Electron"
  ],
  "author": "Lucas Overland",
  "license": "WTFPL",
  "devDependencies": {
    "electron-packager": "5.2.1",
    "electron-prebuilt": "0.36.0"
  },
  "dependencies": {
    "angular": "^1.4.8",
    "archiver": "^0.21.0",
    "bluebird": "^3.1.1",
    "bootstrap": "^3.3.6",
    "fs-extra": "^0.26.4",
    "normalize.css": "^3.0.3",
    "rimraf": "^2.5.0",
    "tar": "^2.2.1",
    "targz": "^1.0.1",
    "unzip": "^0.1.11"
  }
}
