    "electron-prebuilt": "0.36.0"
  },
  "dependencies": {
    "angular": "^1.4.8",
    "archiver": "^0.21.0",
    "bluebird": "^3.1.1",
    "bootstrap": "^3.3.6",
    "fs-extra": "^0.26.4",
    "normalize.css": "^3.0.3",
